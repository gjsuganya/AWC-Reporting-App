<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rule_for_meal_price</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
(Amount__c) &gt; 50,
ISBLANK( Business_Guest__c )
)</errorConditionFormula>
    <errorMessage>Business guest must be included</errorMessage>
</ValidationRule>
